{
	"cli": {
		"version": ">= 12.0.0"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"android": {
				"gradleCommand": ":app:assembleDebug"
			}
		},
		"preview": {
			"distribution": "internal",
			"channel": "preview"
		},
		"production": {
			"channel": "production",
			"env": {
				"API_BASE_URL": "${API_BASE_URL}",
				"STRIPE_PUBLISHABLE_KEY": "${STRIPE_PUBLISHABLE_KEY}"
			}
		}
	},
	"submit": {
		"production": {}
	}
}